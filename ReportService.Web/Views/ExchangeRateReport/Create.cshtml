@model ReportService.Structures.ParameterModels.ExchangeRateParametersModel
@{
    ViewData["Title"] = "Exchange rates report";
}
<h2>Введите параметры для отчета</h2>
<form method="post" asp-antiforgery="true">
       <p>
           <label>Начальная дата: </label>
           <input type="date" name="startDate" />
       </p>
       <p>
            <label>Конечная дата: </label>
            <input type="date" name="endDate" />
       </p>
       <p>
        <label>Включенные валюты: </label>
        <select multiple  name="currencies">
            @foreach(var item in ViewBag.Currencies)
            {
                <option name="@item">@item</option>
            }
        </select>
       </p>
    <input type="submit" value="Сгенерировать" />
</form>


